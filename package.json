{
  "name": "test-project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.json .",
    "start": "NODE_PATH=. bin/www",
    "debug": "NODE_PATH=. nodemon --trace-warnings --inspect=0.0.0.0 bin/www",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "test": "TZ=UTC jest --silent --detectOpenHandles --testPathPattern app/",
    "test:functional": "jest --detectOpenHandles --forceExit test/functional/*"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "dependencies": {
    "@babel/core": "~7.14.6",
    "@babel/plugin-transform-modules-commonjs": "~7.14.5",
    "@babel/register": "~7.14.5",
    "axios": "~0.21.1",
    "cookie-parser": "~1.4.5",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "express-json-validator-middleware": "~2.1.1",
    "http-errors": "~1.7.3",
    "knex": "^0.21.21",
    "lodash": "~4.17.21",
    "nconf": "~0.10.0",
    "objection": "^2.2.15",
    "pg": "~8.6.0"
  },
  "devDependencies": {
    "debug": "~4.3.1",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-json": "~3.0.0",
    "eslint-plugin-prettier": "~3.4.0",
    "jest": "~26.6.3",
    "jest-json-schema": "~5.0.0",
    "nock": "~13.1.1",
    "nodemon": "~2.0.12",
    "prettier": "~2.3.2",
    "supertest": "~6.1.3",
    "timekeeper": "~2.2.0"
  }
}
